    "def last(n):\n",
    "    return n[-1]\n",
    "def sort(d):\n",
    "    return sorted(d,key=last)\n",
    "d=[(2,5),(1,2),(4,4),(2,3),(2,1)]\n",
    "print(\"result:\")\n",
    "print(sort(d))"
